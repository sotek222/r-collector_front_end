{
  "name": "r-collector_front_end",
  "version": "1.0.0",
  "description": "A webapp that lets users display their collections of music",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch",
    "build": "webpack --config=webpack.prod.js",
    "start": "webpack-dev-server --config=webpack.dev.js --open"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sotek222/r-collector_front_end.git"
  },
  "author": "Matthew Masiello",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sotek222/r-collector_front_end/issues"
  },
  "homepage": "https://github.com/sotek222/r-collector_front_end#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.8.4",
    "cowsay": "^1.4.0",
    "css-loader": "^3.2.0",
    "dotenv-webpack": "^1.7.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^1.0.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "@babel/core": "^7.8.4",
    "babel-jest": "^25.1.0",
    "identity-obj-proxy": "^3.0.0",
    "spotify-web-api-js": "^1.2.0"
  }
}
